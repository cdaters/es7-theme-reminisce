      <storyboard>
        <animation property="opacity" from="0" to="0" duration="1" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="600" mode="easeInOut"/>
      </storyboard>
    </text>
    <datetime name="md_releasedate">
      <storyboard>
        <animation property="opacity" from="0" to="0" duration="1" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="600" mode="easeInOut"/>
      </storyboard>
    </datetime>
  </customView>
  -->
  <!-- Modern View routines -->
  <customView name="modern" inherits="grid">
    <ninepatch name="gridtile:selected">
      <!-- thanks to Fabrice CARUSO and his Carbon theme for showing me how to get the tile border to flash -->
      <animateColor>ffffff00</animateColor>
      <animateColorTime>800</animateColorTime>
    </ninepatch>
    <image name="md_marquee">
      <storyboard>
        <animation property="opacity" from="0" to="0" duration="1" mode="linear"/>
        <animation property="opacity" from="0" to="1" begin="200" duration="500" mode="easeOut"/>
