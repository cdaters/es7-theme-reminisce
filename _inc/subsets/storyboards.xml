        <animation property="y" from="0.35" begin="300" duration="600" mode="easeOut"/>
      </storyboard>
    </text>
    <text name="meta-genreReleaseyearPlayers">
      <storyboard>
        <animation property="opacity" from="0" to="0" duration="1" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="600" mode="easeOut"/>
      </storyboard>
    </text>
    <image name="logo2" extra="true">
      <storyboard>
        <animation property="y" from="0.14" begin="300" duration="500" mode="easeOut"/>
        <animation property="opacity" from="0" to="0" duration="300" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="300" mode="easeOut"/>
      </storyboard>
    </image>
    <image name="primaryBtn" extra="true">
      <storyboard>
        <animation property="y" from="0.396481481481481" begin="300" duration="500" mode="easeOut"/>
        <animation property="opacity" from="0" to="0" duration="300" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="300" mode="easeOut"/>
      </storyboard>
    </image>
    <image name="secondaryBtn" extra="true">
      <storyboard>
        <animation property="y" from="0.396481481481481" begin="300" duration="500" mode="easeOut"/>
        <animation property="opacity" from="0" to="0" duration="300" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="300" mode="easeOut"/>
      </storyboard>
    </image>
    <image name="buttonBG" extra="true">
      <storyboard>
        <animation property="y" from="0.396481481481481" begin="300" duration="500" mode="easeOut"/>
        <animation property="opacity" from="0" to="0" duration="300" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="300" mode="easeOut"/>
      </storyboard>
    </image>
    <image name="selectTxt" extra="true">
      <storyboard>
        <animation property="opacity" from="0" to="0" duration="300" mode="linear"/>
        <animation property="opacity" from="0" to="1" duration="500" begin="500" mode="easeOut"/>
      </storyboard>
    </image>
    <imagegrid name="gamegrid">
      <storyboard>
        <animation property="opacity" from="0" to="1" duration="800" mode="easeOut"/>
        <animation property="y" from="0.657037037037037" duration="800" mode="easeOut"/>
      </storyboard>
    </imagegrid>
  </customView>
  <feature supported="manufacturer">
    <view name="system">
      <text name="releaseYear" extra="true">
        <storyboard>
          <animation property="opacity" from="0" to="0" duration="300" mode="linear"/>
          <animation property="opacity" from="0" to="0.2" duration="500" begin="300" mode="easeOut"/>
        </storyboard>
      </text>
    </view>
  </feature>
</theme>
